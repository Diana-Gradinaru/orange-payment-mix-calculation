  <Sequence sap2010:Annotation.AnnotationText="Filters the data in the input excel file as such:&#xA;- removes rows in columns &quot;CM MATERNITATE&quot; and &quot;CM RISC MATERNAL&quot; where the value is greater than 0&#xA;- remove rows in column &quot;NR ORE LUNA&quot; where the value is 0&#xA;- only keep a limited number of columns" DisplayName="FilterInputFile" sap:VirtualizedContainerService.HintSize="558.4,886.4" sap2010:WorkflowViewState.IdRef="Sequence_1">
    <sap:WorkflowViewStateService.ViewState>
      <scg:Dictionary x:TypeArguments="x:String, x:Object">
        <x:Boolean x:Key="IsExpanded">True</x:Boolean>
        <x:Boolean x:Key="IsAnnotationDocked">True</x:Boolean>
      </scg:Dictionary>
    </sap:WorkflowViewStateService.ViewState>
    <ui:LogMessage DisplayName="Log Message" sap:VirtualizedContainerService.HintSize="516.8,92.8" sap2010:WorkflowViewState.IdRef="LogMessage_1" Level="Info" Message="[&quot;Start FilterInputFile&quot;]" />
    <Assign DisplayName="Assign filtered data table" sap:VirtualizedContainerService.HintSize="516.8,64" sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="sd:DataTable">[out_dtFilteredTable]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable" xml:space="preserve">[(From r In in_dtInputFile.Select()
Where r("CM MATERNITATE").ToString.Equals("0") And r("CM RISC MATERNAL").ToString.Equals("0") _
And Not r("NR ORE LUNA").ToString.Equals("0")
Select r).CopyToDataTable]</InArgument>
      </Assign.Value>
    </Assign>
    <ui:ForEachRow ColumnNames="{x:Null}" CurrentIndex="{x:Null}" DataTable="[out_dtFilteredTable]" DisplayName="For Each Row in Filtered Data Table" sap:VirtualizedContainerService.HintSize="516.8,364.8" sap2010:WorkflowViewState.IdRef="ForEachRow_2">
      <ui:ForEachRow.Body>
        <ActivityAction x:TypeArguments="sd:DataRow">
          <ActivityAction.Argument>
            <DelegateInArgument x:TypeArguments="sd:DataRow" Name="row" />
          </ActivityAction.Argument>
          <If Condition="[Cint(row(&quot;PARTE VARIABILA NET&quot;)) &gt; 0 and Cint(row(&quot;PARTE VARIABILA NET&quot;))  &lt; 99.99]" DisplayName="If row &quot;PARTE VARIABILA NET&quot; has values between 0 and 99.99" sap:VirtualizedContainerService.HintSize="483.2,248.8" sap2010:WorkflowViewState.IdRef="If_3">
            <If.Then>
              <Assign DisplayName="Assign value 0 to row" sap:VirtualizedContainerService.HintSize="262.4,60" sap2010:WorkflowViewState.IdRef="Assign_4">
                <Assign.To>
                  <OutArgument x:TypeArguments="x:Object">[row("PARTE VARIABILA NET")]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="x:Object">[0]</InArgument>
                </Assign.Value>
              </Assign>
            </If.Then>
            <If.Else>
              <Sequence sap2010:WorkflowViewState.IdRef="Sequence_9">
                <sap:WorkflowViewStateService.ViewState>
